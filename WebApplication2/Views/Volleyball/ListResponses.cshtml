@model IEnumerable<PartyInvites.Models.VolleyBallResponse>
@{ Layout = null;}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="/css/styles.css" />
    <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css" />
    <title>Responses</title>
</head>
<body>
    <div class="panel-body">
    <h2>Here are the other people attending this event!</h2>
    <table class="table table-sm table-striped table-bordered">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>ContactNo</th>
            </tr>
        </thead>
        <tbody>
            @foreach (PartyInvites.Models.VolleyBallResponse r in Model)
            {
                <tr>
                    <td>@r.Name</td>
                    <td>@r.Email</td>
                    <td>@r.ContactNo</td>
                </tr>

            }

        </tbody>
    </table>
    </div>
    <div class="text-center">
        <a class="btn btn-primary" href="@Url.Action("Index","Home")">Home Page</a>
    </div>
</body>
</html>

